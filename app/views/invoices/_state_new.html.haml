=render :partial => 'steps'

=form_for [@project, @invoice] do |f|
  -if @invoice.errors.any?
    #error_explanation
      %h2= "#{pluralize(@invoice.errors.count, "error")} prohibited this labor_cost from being saved:"
      %ul
        - @invoice.errors.full_messages.each do |msg|
          %li= msg
          
  .field
  = f.label :date
  = f.date_select :date
  
  .field
  = f.submit 'Next'
