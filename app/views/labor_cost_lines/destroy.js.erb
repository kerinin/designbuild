<% content_for :overview_list do %>
  <% case params[:by] %>
  <% when nil, 'project' %>
    <%= render :partial => 'overview_by_project', :locals => {:labor_cost => @labor_cost, :line_items => @labor_cost} %>
  <% when 'employee' %>
    <%= render :partial => 'overview_by_employee', :locals => {:employee => @labor_cost_line.laborer, :date => @labor_cost_line.labor_set.date} %>
  <% when 'date' %>
    <%= render :partial => 'overview_by_employee', :locals => {:employee => @labor_cost_line.laborer, :date => @labor_cost_line.labor_set.date} %>
  <% end %>
<% end %>

$("#overview_list_<%= @labor_cost.id %>").replaceWith('<%= escape_javascript( yield :overview_list ) %>');