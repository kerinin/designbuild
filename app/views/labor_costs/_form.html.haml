= form_for @labor_cost do |f|
  -if @labor_cost.errors.any?
    #error_explanation
      %h2= "#{pluralize(@labor_cost.errors.count, "error")} prohibited this labor_cost from being saved:"
      %ul
        - @labor_cost.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :date
    = f.date_select :date
  .actions
    = f.submit 'Save'
