<% content_for :insert do %>
  <% case params[:by] %>
  <% when nil, 'project' %>
    <%= render :partial => 'by_project' %>
  <% when 'employee' %>
    <%= render :partial => 'by_employee' %>
  <% when 'date' %>
    <%= render :partial => 'by_date' %>
  <% end %>
<% end %>

$('.labor_costs').replaceWith('<%= escape_javascript( yield :insert ) %>');